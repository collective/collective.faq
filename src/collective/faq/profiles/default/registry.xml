<?xml version="1.0" encoding="utf-8"?>
<registry>
  <record field="displayed_types"
          interface="Products.CMFPlone.interfaces.controlpanel.INavigationSchema"
          name="plone.displayed_types"
  >
    <field type="plone.registry.field.Tuple">
      <default>
        <element>Image</element>
        <element>File</element>
        <element>Link</element>
        <element>News Item</element>
        <element>Folder</element>
        <element>Document</element>
        <element>Event</element>
      </default>
      <description xmlns:ns0="http://xml.zope.org/namespaces/i18n"
                   ns0:domain="plone"
                   ns0:translate=""
      >The content types that should be shown in the navigation and site map.</description>
      <missing_value />
      <required>False</required>
      <title xmlns:ns0="http://xml.zope.org/namespaces/i18n"
             ns0:domain="plone"
             ns0:translate=""
      >Displayed content types</title>
      <value_type type="plone.registry.field.Choice">
        <vocabulary>plone.app.vocabularies.ReallyUserFriendlyTypes</vocabulary>
      </value_type>
    </field>
    <value purge="False">
      <element>FAQ</element>
    </value>
  </record>

  <records interface="Products.CMFPlone.interfaces.IResourceRegistry"
           prefix="plone.resources/collective-faq"
  >
    <value key="js">++resource++collective.faq/faq.js</value>
    <value key="deps">jquery</value>
    <value key="css">
      <element>++resource++collective.faq/faq.css</element>
    </value>
  </records>

  <records interface="Products.CMFPlone.interfaces.IBundleRegistry"
           prefix="plone.bundles/collective-faq"
  >
    <value key="resources"
           purge="false"
    >
      <element>collective-faq</element>
    </value>
    <value key="enabled">True</value>
    <value key="csscompilation">++plone++static/collective-faq.min.css</value>
    <value key="jscompilation">++plone++static/collective-faq.min.js</value>
    <value key="compile">False</value>
  </records>

</registry>
